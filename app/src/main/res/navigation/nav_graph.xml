<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_graph"
  app:startDestination="@id/mainFragment">
  <fragment
    android:id="@+id/mainFragment"
    android:label="MainFragment"
    android:name="com.tink.link.MainFragment"
    tools:layout="@layout/fragment_main">
    <action
      android:id="@+id/action_mainFragment_to_providerListFragment"
      app:destination="@id/providerListFragment"
      app:popUpTo="@+id/nav_graph" />
  </fragment>
  <fragment
    android:id="@+id/providerListFragment"
    android:name="com.tink.link.providerlist.ProviderListFragment"
    tools:layout="@layout/layout_provider_tree_node_list">
    <action
      android:id="@+id/action_providerListFragment_to_financialInstitutionListFragment"
      app:destination="@id/financialInstitutionListFragment" />
  </fragment>
  <fragment
    android:id="@+id/financialInstitutionListFragment"
    android:label="FinancialInstitutionListFragment"
    android:name="com.tink.link.financialinstitution.FinancialInstitutionListFragment"
    tools:layout="@layout/fragment_financial_institution_list">
    <action
      android:id="@+id/action_financialInstitutionListFragment_to_accessTypeListFragment"
      app:destination="@id/accessTypeListFragment" />
  </fragment>
  <fragment
    android:id="@+id/accessTypeListFragment"
    android:label="AccessTypeListFragment"
    android:name="com.tink.link.accesstype.AccessTypeListFragment"
    tools:layout="@layout/fragment_access_type_list">
    <action
      android:id="@+id/action_accessTypeListFragment_to_credentialTypeListFragment"
      app:destination="@id/credentialTypeListFragment" />
  </fragment>
  <fragment
    android:id="@+id/credentialTypeListFragment"
    android:label="CredentialTypeListFragment"
    android:name="com.tink.link.credentialtype.CredentialTypeListFragment"
    tools:layout="@layout/fragment_credential_type_list">
    <action
      android:id="@+id/action_credentialTypeListFragment_to_providerNodeFragment"
      app:destination="@id/providerNodeFragment" />
  </fragment>
  <fragment
    android:id="@+id/credentialFragment"
    android:label="CredentialFragment"
    android:name="com.tink.link.credentials.CredentialFragment"
    tools:layout="@layout/fragment_credential" />
  <fragment
    android:id="@+id/providerNodeFragment"
    android:name="com.tink.link.providernode.ProviderNodeFragment"
    android:label="ProviderNodeFragment"
    tools:layout="@layout/fragment_provider_node">
    <action
      android:id="@+id/action_providerNodeFragment_to_credentialFragment"
      app:destination="@id/credentialFragment" />
  </fragment>
</navigation>